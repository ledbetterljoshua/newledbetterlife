<h1>Listing about_mes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_mes.each do |about_me| %>
      <tr>
        <td><%= about_me.description %></td>
        <td><%= about_me.title %></td>
        <td><%= link_to 'Show', about_me %></td>
        <td><%= link_to 'Edit', edit_about_me_path(about_me) %></td>
        <td><%= link_to 'Destroy', about_me, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About me', new_about_me_path %>
