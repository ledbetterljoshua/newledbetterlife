<h1>Listing bannerimages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bannerimages.each do |bannerimage| %>
      <tr>
        <%= image_tag bannerimage.image.url(:original, timestamp: false) %>
        <td><%= link_to 'Show', bannerimage %></td>
        <td><%= link_to 'Edit', edit_bannerimage_path(bannerimage) %></td>
        <td><%= link_to 'Destroy', bannerimage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bannerimage', new_bannerimage_path %>
